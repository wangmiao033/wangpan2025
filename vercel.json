{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/file",
      "dest": "/api/file.js"
    },
    {
      "src": "/api/upload",
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/download/(.*)",
      "dest": "/api/download.js?code=$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}